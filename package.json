{
    "name": "omichat",
    "version": "1.0.0",
    "description": "Serverless webpack example using Typescript",
    "main": "handler.js",
    "scripts": {
        "prestart": "rm -rf .build",
        "start": "serverless offline --stage dev --host 0.0.0.0 ",
        "test": "jest --watch --runInBand --verbose",
        "deploy": "serverless deploy --stage product",
        "tail": "serverless logs -f graphql --tail -F --stage product",
        "removeService": "serverless remove --stage product"
    },
